{
  "name": "medical-appointment-system",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean && rimraf node_modules"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "turbo": "^2.0.0"
  },
  "packageManager": "npm@10.2.3",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "framer-motion": "^12.15.0"
  }
}
